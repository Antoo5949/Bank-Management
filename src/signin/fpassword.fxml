<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import java.net.URL?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="signin.FpasswordController">
    <stylesheets>
        <URL value="@fpassword.css" />
    </stylesheets>
   <children>
      <TextField fx:id="fpemail" layoutX="210.0" layoutY="101.0" promptText="Email Id">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <TextField fx:id="fpnpass" layoutX="213.0" layoutY="162.0" onKeyPressed="#dischnge" promptText="New Password">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <TextField fx:id="fpcpass" layoutX="213.0" layoutY="221.0" onKeyReleased="#dischnge" promptText="Confirm Password">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Label alignment="CENTER" layoutX="23.0" layoutY="36.0" prefHeight="51.0" prefWidth="512.0" text="Please Insert Your Email Id">
         <font>
            <Font name="System Bold" size="27.0" />
         </font>
      </Label>
      <JFXButton fx:id="passchange" layoutX="262.0" layoutY="285.0" onAction="#passchange" text="Change" />
      <JFXButton fx:id="passearch" layoutX="416.0" layoutY="104.0" onAction="#passearch" prefHeight="25.0" prefWidth="64.0" text="Search" />
   </children>
</AnchorPane>
